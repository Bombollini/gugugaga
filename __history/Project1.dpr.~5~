program Project1;

uses
  Forms,
  Praktikum1 in 'Praktikum1.pas' {Form1},
  Praktikum2 in 'Praktikum2.pas' {Form2},
  Vcl.Themes,
  Vcl.Styles,
  Praktikum3 in 'Praktikum3.pas' {Form3},
  Praktikum4 in 'Praktikum4.pas' {Form4},
  Praktikum5 in 'Praktikum5.pas' {Form5},
  Praktikum6 in 'Praktikum6.pas' {Form6};

{$R *.res}

begin
  Application.Initialize;
  TStyleManager.TrySetStyle('Glossy');
  Application.CreateForm(TForm6, Form6);
  Application.CreateForm(TForm5, Form5);
  Application.CreateForm(TForm4, Form4);
  Application.CreateForm(TForm3, Form3);
  Application.CreateForm(TForm2, Form2);
  Application.CreateForm(TForm1, Form1);
  Application.Run;
end.
